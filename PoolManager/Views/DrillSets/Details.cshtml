@model PoolManager.ViewModels.DrillSetsFormViewModel
@{
    ViewBag.Title = "Details";
}

<h2>@Model.Heading</h2>


<dt>Drills: </dt>
<dd>
    @foreach (var item in Model.Drills)
    {
        if (item.Checked)
        {
            <li> @item.Name</li>
        }
    }
</dd>

@Html.Raw(Model.Description)

